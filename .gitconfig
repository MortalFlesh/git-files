[user]
	useconfigonly = true
[push]
	default = tracking
[core]
	excludesfile = /Users/chromecp/.gitignore_global
	editor = sublime -n -w
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[alias]
	up = "!git remote update -p; git merge --ff-only @{u}"
	cif = =
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green bold
[color "diff"]
  meta = normal bold
  frag = magenta bold
  old = red bold
  new = cyan bold
[color "status"]
  branch = yellow bold
  added = green
  changed = cyan
  untracked = red
