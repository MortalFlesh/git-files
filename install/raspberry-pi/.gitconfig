[user]
  useconfigonly = true
[push]
  default = tracking
[core]
  excludesfile = ~/.gitignore_global
  ignorecase = false
[merge]
    conflictstyle = diff3
[alias]
	up = "!git remote update -p; git merge --ff-only @{u}"
  cif = =
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green bold
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
  context = normal dim
[color "status"]
  branch = yellow bold
  added = green
  changed = cyan
  untracked = red
[credential]
  helper = cache --timeout=86400
